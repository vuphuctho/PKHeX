<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Width="600" Height="600" CanResize="False"
        x:Class="PKHeX.Avalonia.Views.AboutView"
        Title="About">
    <TabControl HorizontalAlignment="Left">
        <TabItem>
            <TabItem.Header>
                <TextBlock FontSize="16">Shortcuts</TextBlock>
            </TabItem.Header>
            <ScrollViewer>
                <TextBlock>TBU</TextBlock>
            </ScrollViewer>
        </TabItem>
        <TabItem IsSelected="True">
            <TabItem.Header>
                <TextBlock FontSize="16">Changelogs</TextBlock>
            </TabItem.Header>
            <ScrollViewer>
                <TextBlock>TBU 2</TextBlock>
            </ScrollViewer>
        </TabItem>
    </TabControl>
</Window>

